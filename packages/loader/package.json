{
  "name": "@yascml/loader",
  "version": "1.0.0",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build:full": "vite build --config config/vite.config.full.ts --mode development",
    "build:full:prod": "tsc && vite build --config config/vite.config.full.ts --mode production",
    "build:nolib": "vite build --config config/vite.config.nolib.ts --mode development",
    "build:nolib:prod": "vite build --config config/vite.config.nolib.ts --mode production",
    "build": "npm run build:full && npm run build:nolib",
    "build:prod": "npm run build:full:prod && npm run build:nolib:prod"
  },
  "devDependencies": {
    "typescript": "~5.9.2",
    "vite": "^7.1.5",
    "unplugin-dts": "~1.0.0-beta.6",
    "@types/node": "~24.5.2",
    "@types/twine-sugarcube": "~2.37.2",
    "@yascml/types": "workspace:*",
    "@types/semver": "~7.7.1",
    "@types/spark-md5": "~3.0.5"
  },
  "dependencies": {
    "jszip": "~3.10.1",
    "@yascml/utils": "workspace:*",
    "idb-keyval": "~6.2.2",
    "semver": "~7.7.2",
    "spark-md5": "~3.0.2"
  }
}
